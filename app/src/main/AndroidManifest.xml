<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mgp_uy1">

    <application
        android:name=".EHEApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EHESettingsActivity"
            android:label="@string/title_activity_kim_settings"
            android:parentActivityName=".EHEMainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mgp_uy1.EHEMainActivity" />
        </activity>
        <activity
            android:name=".EHEAddAssessmentActivity"
            android:label="@string/title_activity_kim_add_assessment"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EHEAddCourseActivity"
            android:label="@string/title_activity_kim_add_course"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EHEMyAssessmentsActivity"
            android:label="@string/title_activity_kim_my_assessments" />
        <activity
            android:name=".EHEMainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>